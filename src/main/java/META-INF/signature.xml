<entity-mappings
  xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
  version="2.0">
	<entity class="fr.symphonie.tools.signature.model.Signature">
		<table name="DroitSignature">
		</table>
		<attributes>
			<id name="id">
				<column column-definition="numeric identity not null"></column>
				<generated-value strategy="IDENTITY"></generated-value>
			</id>
			<basic name="codeUtil">
				<column name="code_util" length="10" nullable="false"></column>
			</basic>
			<basic name="startDate">
				<column name="date_debut"></column>
				<temporal>DATE</temporal>
			</basic>
			<basic name="endDate">
				<column name="date_fin"></column>
				<temporal>DATE</temporal>
			</basic>
			<basic name="function">
				<column name="fonction"></column>
			</basic>
			<basic name="signatureData">
				<column name="img_signature" column-definition="image"></column>
			</basic>
			<basic name="sf">
				<column name="valid_sf"></column>
			</basic>
			<basic name="dp">
				<column name="valid_dp"></column>
			</basic>
			<basic name="dr">
				<column name="valid_dr"></column>
			</basic>
			<basic name="dv">
				<column name="valid_dv"></column>
			</basic>
			<basic name="lrec">
				<column name="valid_lrec"></column>
			</basic>
			<basic name="seuilLrec">
				<column name="seuil_lrec" column-definition="money default 0"></column>
			</basic>
			<basic name="ar">
				<column name="valid_ar"></column>
			</basic>
			<basic name="seuilAr">
				<column name="seuil_ar" column-definition="money default 0"></column>
			</basic>
			<basic name="da">
				<column name="valid_da"></column>
			</basic>
			<basic name="seuilDa">
				<column name="seuil_da" column-definition="money default 0"></column>
			</basic>
			<basic name="ej">
				<column name="valid_ej"></column>
			</basic>
			<basic name="seuilEj">
				<column name="seuil_ej" column-definition="money default 0"></column>
			</basic>
			<basic name="asf">
				<column name="valid_asf"></column>
			</basic>
				<basic name="seuilAsf">
				<column name="seuil_asf" column-definition="money default 0"></column>
			</basic>
			<basic name="vsf">
				<column name="valid_vsf"></column>
			</basic>
			<basic name="seuilVsf">
				<column name="seuil_vsf" column-definition="money default 0"></column>
			</basic>
			<basic name="liq">
				<column name="valid_liq"></column>
			</basic>
			<basic name="seuilLiq">
				<column name="seuil_liq" column-definition="money default 0"></column>
			</basic>
			<basic name="liqOr">
				<column name="valid_or"></column>
			</basic>
		   <basic name="seuilOr">
				<column name="seuil_or" column-definition="money default 0"></column>
			</basic>
			<basic name="op">
				<column name="valid_op"></column>
			</basic>		
			<basic name="seuilOp">
				<column name="seuil_op" column-definition="money default 0"></column>
			</basic>
			<basic name="mdt">
				<column name="emis_mdt"></column>
			</basic>		
			<basic name="seuilMdt">
				<column name="seuil_mdt" column-definition="money default 0"></column>
			</basic>
			<basic name="tr">
				<column name="emis_tr"></column>
			</basic>		
			<basic name="seuilTr">
				<column name="seuil_tr" column-definition="money default 0"></column>
			</basic>
			<embedded name="trace">
			</embedded>
		</attributes>
	</entity>
  
  </entity-mappings>